{
    "compilerOptions": {
      "jsx": "react-jsx", 
      "allowJs": true,
      "checkJs": false, 
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "module": "esnext",
      "target": "esnext",
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": [
      "src/**/*"
    ] 
  }
  